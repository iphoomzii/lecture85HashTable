<Window x:Class="HashTable.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashTable"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
    <TextBlock HorizontalAlignment="Center" VerticalAlignment="top" Width="300" Height="40" Margin="80">KEY: </TextBlock>
    <TextBox x:Name="key" HorizontalAlignment="Center" VerticalAlignment="top" Width="300" Height="40" Margin="100"></TextBox>
    <TextBlock HorizontalAlignment="Center" VerticalAlignment="top" Width="300" Height="40" Margin="140">VALUE: </TextBlock>
    <TextBox x:Name="value" HorizontalAlignment="Center" VerticalAlignment="top" Width="300" Height="40" Margin="160"></TextBox>

    <Button Click="add" HorizontalAlignment="Left" VerticalAlignment="bottom" Margin="160" Width="60" Height="30">Add</Button>

    <Button Click="show" HorizontalAlignment="Center" VerticalAlignment="bottom" Margin="160" Width="60" Height="30">Show</Button>

    <Button Click="remove" HorizontalAlignment="Right" VerticalAlignment="bottom" Margin="160" Width="60" Height="30">Remove</Button>

    </Grid>
</Window>
